vim_rm_lines() {
# 1 = the search phrase
    # 2 = number of line before
    # 3 = number of lines after
    # 4 = input file
    # 5 = output file
    vim -Nes "+g/$1/.-$2,.+$3d" '+w !tee' '+q!' $4 > $5
}

